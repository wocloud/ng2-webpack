{
  "name": "n2-webpack",
  "version": "1.0.0",
  "description": "angularjs 2 and webpack project.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "lint": "tslint ./src/**/*.ts",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start": "concurrently \"npm run build:watch\" \"npm run server\""
  },
  "keywords": [
    "Angular 2",
    "webpack"
  ],
  "author": "sophia.wang",
  "license": "MIT",
  "devDependencies": {
    "@angularclass/hmr-loader": "^3.0.2",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.0.7",
    "concurrently": "^3.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.1",
    "tslib": "^1.6.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "bulma": "^0.3.2",
    "codelyzer": "^2.0.1",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wocloud/ng2-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/wocloud/ng2-webpack/issues"
  },
  "homepage": "https://github.com/wocloud/ng2-webpack#readme"
}
